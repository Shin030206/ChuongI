    "def factorial(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    return n*factorial(n-1)\n",
    "\n",
    "print('5!=',factorial(5))\n",
    "\n",
    "def H10toH2(n):\n",
    "    if n>0:\n",
    "        sd=n%2\n",
    "        n=n//2\n",
    "        H10toH2(n)\n",
    "        print(sd,end=' ')\n",
    "n=33\n",
    "H10toH2(n)"
   ]
